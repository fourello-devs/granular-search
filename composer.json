{
    "name": "fourello-devs/granular-search",
    "description": "Granular Search is a package for Laravel which aims to provide automatic searching feature to Eloquent models.",
    "license": "MIT",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "carlo.luchavez@fourello.com",
            "homepage": "https://github.com/luchmewep"
        }
    ],
    "homepage": "https://github.com/fourello-devs/granular-search",
    "keywords": ["Laravel", "GranularSearch"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "FourelloDevs\\GranularSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FourelloDevs\\GranularSearch\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FourelloDevs\\GranularSearch\\GranularSearchServiceProvider"
            ],
            "aliases": {
                "GranularSearch": "FourelloDevs\\GranularSearch\\Facades\\GranularSearch"
            }
        }
    }
}
